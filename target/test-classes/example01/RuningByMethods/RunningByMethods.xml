<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Running By Methods Suite">
	<test name="SocialWebLink TestCase">
		<classes>
			<class name="example01.RuningByMethods.SocialWebLink">
				<methods>
					<include name="facebookTestCase" />
					<include name="linkedinTestCase" />
					<include name="twitterTestCase" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="SearchEngine TestCase">
		<classes>
			<class name="example01.RuningByMethods.SearchEngine">
				<methods>
					<include name="googleTestCase" />
					<include name="bingTestCase" />
					<include name="yandexTestCase" />
				</methods>
			</class>
		</classes>
	</test>
</suite>
